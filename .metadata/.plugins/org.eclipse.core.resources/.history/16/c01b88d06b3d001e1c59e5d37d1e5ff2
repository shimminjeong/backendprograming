<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.BoardDAO">
	<insert id="insertNewPost">
		insert into t_board(no, title, writer,content)
		values(seq_t_board.nextval,'오늘의 날씨', '심민정','안녕하슈')
	</insert>

	<insert id="insertNewPost2" parameterType="mybatis.BoardVO">
		insert into t_board(no, title, writer,content)
		values(seq_t_board.nextval,#{title},#{writer},#{content})
	</insert>

	<select id="selectAll" resultSetType="mybatis.BoardVO">
		select * from t_board
		order by no desc;
	</select>



</mapper>